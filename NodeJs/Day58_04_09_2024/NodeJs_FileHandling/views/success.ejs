<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>File Upload Successful</title>
</head>
<body>
    <h1>Files Uploaded Successfully</h1>
    <ul>
        <% filenames.forEach(function(filename) { %>
            <li>File: <%= filename %></li>
        <% }); %>
    </ul>
</body>
</html>
